modules = ["nodejs-20", "web", "python-3.11"]
[agent]
expertMode = true
integrations = ["notion:1.0.0", "github:1.0.0"]

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Development Server"

[[workflows.workflow]]
name = "Development Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd \"QueryLinker (2)\" && npm run dev"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 5001
externalPort = 3002

[[ports]]
localPort = 9229
externalPort = 3000

[[ports]]
localPort = 9230
externalPort = 3001

[[ports]]
localPort = 35219
externalPort = 3003

[deployment]
deploymentTarget = "autoscale"
run = ["npx", "wrangler", "pages", "dev", "dist", "--port", "8080"]
build = ["npm", "run", "build"]
